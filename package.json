{
  "name": "codeless",
  "displayName": "Codeless",
  "description": "Process files without writing code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeless.process",
        "title": "Process Open Files",
        "category": "Codeless"
      }
    ],
    "keybindings": [
    {
      "command": "codeless.process",
      "key": "alt+e",
      "mac": "alt+e",
      "when": "editorTextFocus"
    }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^14.x.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.x.x",
    "@typescript-eslint/parser": "^4.x.x",
    "eslint": "^7.x.x",
    "typescript": "^4.x.x"
  },
  "dependencies": {
    "openai": "^4.52.7"
  }
}